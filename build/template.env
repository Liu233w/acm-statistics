# == docker compose 参数 ==

# 监听的端口
EXPOSE_PORT=3000
# traefik 的监听端口，不建议暴露给外部
TRAEFIK_PORT=3001

# 镜像名和标签，不需要修改
DOCKER_REPO=
DOCKER_TAG=

# == 内部程序的参数 ==
# vjudge 爬虫用到的，填你自己的即可
VJUDGE_USERNAME=vjudge用户名
VJUDGE_PASSWORD=vjudge密码

# MySQL root 的密码
MYSQL_ROOT_PASSWORD=password

# backend admin 的默认密码
BACKEND_ADMIN_DEFAULT_PASSWORD=123qwe

# 后端jwt私钥
BACKEND_JWT_PRIVATE_KEY=AcmStatisticsBackend_C421AAEE0D114E9C

# 数据库备份UID，如果你平常都是用root来访问服务器的，以下的uid设为0即可。否则设置为你自己的uid
DB_BACKUP_UID=0
