{
  "name": "acm-statistics-frontend",
  "version": "1.0.0",
  "description": "ACM查题网站前端",
  "author": "Liu233w <wwwlsmcom@outlook.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue .",
    "lintfix": "eslint --fix --ext .js,.vue .",
    "analyze": "nuxt build --analyze",
    "test": "jest __test__/",
    "snapshot": "jest __test__/ --watch",
    "postinstall": "node -e \"var s='../crawler',d='./node_modules/crawler',fs=require('fs'), r=require('path').resolve;fs.exists(d,function(e){e||fs.symlinkSync(r(s),r(d),'junction')});\""
  },
  "dependencies": {
    "@nuxtjs/axios": "5.13.1",
    "@nuxtjs/component-cache": "1.1.6",
    "@nuxtjs/vuetify": "1.11.3",
    "chart.js": "2.9.4",
    "cheerio": "1.0.0-rc.5",
    "html2canvas": "1.0.0-rc.7",
    "js-cookie": "2.2.1",
    "lodash": "4.17.21",
    "nuxt": "2.14.12",
    "nuxt-env": "0.1.0",
    "stylus": "0.54.8",
    "stylus-loader": "5.0.0",
    "superagent": "5.3.1",
    "virtual-module-webpack-plugin": "0.4.1",
    "vue-chartjs": "3.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.12.17",
    "@vue/test-utils": "1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-lodash": "3.3.4",
    "eslint": "7.21.0",
    "eslint-config-standard": "16.0.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-html": "6.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-lodash": "7.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-vue": "7.6.0",
    "eslint-plugin-vuetify": "1.0.0-beta.7",
    "jest": "26.6.3",
    "jest-serializer-vue": "2.0.2",
    "vue-jest": "3.0.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "~(?!/dynamic)(.*)$": "<rootDir>/$1"
    },
    "setupFilesAfterEnv": ["<rootDir>/__test__/setup.js"],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
