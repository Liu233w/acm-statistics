{
  "name": "acm-statistics-frontend",
  "version": "1.0.0",
  "description": "ACM查题网站前端",
  "author": "Liu233w <gh@liu233w.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue .",
    "lintfix": "eslint --fix --ext .js,.vue .",
    "analyze": "nuxt build --analyze",
    "test": "jest __test__/",
    "snapshot": "jest __test__/ --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.5.1",
    "@mdi/font": "^7.4.47",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/component-cache": "1.1.6",
    "chart.js": "4.4.1",
    "cheerio": "1.0.0-rc.12",
    "crawler": "link:../crawler",
    "html2canvas": "1.4.1",
    "js-cookie": "3.0.5",
    "lodash": "4.17.21",
    "nuxt": "3.10.1",
    "nuxt-env": "0.1.0",
    "stylus": "0.62.0",
    "superagent": "8.1.2",
    "vue-chartjs": "5.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@nuxt/kit": "^3.10.1",
    "@nuxtjs/eslint-module": "4.1.0",
    "@vue/test-utils": "2.4.4",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "29.7.0",
    "babel-plugin-lodash": "3.3.4",
    "eslint": "8.56.0",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-lodash": "7.4.0",
    "eslint-plugin-vue": "9.21.1",
    "eslint-plugin-vuetify": "2.1.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-serializer-vue": "3.1.0",
    "stylus-loader": "8.1.0",
    "typescript": "5.3.3",
    "vite-plugin-virtual": "^0.2.0",
    "vite-plugin-vuetify": "^2.0.1",
    "vuetify": "^3.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "@vue/vue2-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "~(?!/dynamic)(.*)$": "<rootDir>/$1"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
