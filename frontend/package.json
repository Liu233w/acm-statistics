{
  "name": "acm-statistics-frontend",
  "version": "1.0.0",
  "description": "ACM查题网站前端",
  "author": "Liu233w <wwwlsmcom@outlook.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue .",
    "lintfix": "eslint --fix --ext .js,.vue .",
    "analyze": "nuxt build --analyze",
    "test": "jest __test__/",
    "snapshot": "jest __test__/ --watch",
    "postinstall": "node -e \"var s='../crawler',d='./node_modules/crawler',fs=require('fs'), r=require('path').resolve;fs.exists(d,function(e){e||fs.symlinkSync(r(s),r(d),'junction')});\""
  },
  "dependencies": {
    "@nuxtjs/component-cache": "1.1.5",
    "@nuxtjs/vuetify": "1.9.0",
    "axios": "0.19.0",
    "cheerio": "1.0.0-rc.3",
    "lodash": "4.17.15",
    "lodash-webpack-plugin": "0.11.5",
    "nuxt": "2.10.2",
    "nuxt-env": "0.1.0",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "superagent": "5.1.2",
    "virtual-module-webpack-plugin": "0.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.7.5",
    "@fortawesome/fontawesome-free": "5.11.2",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "eslint": "6.5.1",
    "eslint-config-standard": "14.1.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.21.0",
    "eslint-plugin-lodash": "6.0.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.0.1",
    "eslint-plugin-vuetify": "1.0.0-beta.5",
    "jest": "24.9.0",
    "jest-serializer-vue": "2.0.2",
    "ps-tree": "1.2.0",
    "vue-jest": "3.0.5"
  },
  "jest": {
    "globals": {
      "vue-jest": {
        "babelConfig": {
          "presets": [
            "@babel/preset-env"
          ]
        }
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/__test__/jest.transform.js",
      ".*\\.(vue)$": "vue-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/__test__/pages/pages_snapshot.test.js"
    ],
    "moduleNameMapper": {
      "~(?!/dynamic)(.*)$": "<rootDir>/$1"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
